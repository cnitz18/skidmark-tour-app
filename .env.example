# Skidmark Tour App - Environment Variables
# Copy this file to .env.local and fill in your values

# Main API - Race data, leagues, OCR endpoints
REACT_APP_AMS2API=http://localhost:8000

# Dedicated game server API - Live session status
REACT_APP_SERVER_LOC=http://localhost:3000/

# App display name (used in page title)
REACT_APP_ENV="Skidmark Tour"

# Twitch API - For detecting live streams on homepage
REACT_APP_TWITCH_CLIENT_ID=
REACT_APP_TWITCH_CLIENT_SECRET=

# Discord bot API - Chorley Console in AdminPortal
REACT_APP_BOT_SERVER_URL=http://localhost:3001
REACT_APP_BOT_SERVER_TOKEN=
